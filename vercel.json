{
    "rewrites": [
        {
            "source": "/api/v1/link",
            "destination": "http://localhost:8000/link"
          }
    ],
    "headers": [
      { 
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Origin, X-Requested-With, Content-Type, Accept"
          }
        ]
      }
    ]
  }
  